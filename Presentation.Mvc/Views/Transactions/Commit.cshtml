@model CommitTransferCommand

@{
    ViewData["Title"] = "Commit Transfer";
}

<nav aria-label="breadcrumb">
    <ul class="breadcrumb">
        <li class="breadcrumb-item"><a href="/">Bank</a></li>
        <li class="breadcrumb-item active" aria-current="page">commit</li>
    </ul>
</nav>

<form asp-action="Commit" method="POST">
    <div class="form-group">
        <input asp-for="TransactionId" type="text" class="form-control" id="TransactionId" aria-describedby="TransactionId" placeholder="Enter transaction id">
    </div>
    <br />
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
<br />
<p>
    <a asp-action="All" class="btn btn-secondary">Back to List</a>
</p>